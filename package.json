    "@vitest/coverage-v8": "0.34.1",
    "@vue/eslint-config-prettier": "8.0.0",
    "@vue/test-utils": "2.2.2",
    "c8": "8.0.1",
    "eslint": "8.46.0",
    "eslint-plugin-vue": "9.16.1",
